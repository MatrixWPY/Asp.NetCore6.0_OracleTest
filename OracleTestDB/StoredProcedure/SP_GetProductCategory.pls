CREATE OR REPLACE PROCEDURE SP_GETPRODUCTCATEGORY(
    i_categoryId NUMBER,
    o_result OUT SYS_REFCURSOR
)
AS 
BEGIN

    OPEN o_result FOR
    SELECT
        CATEGORY_ID,
        CATEGORY_NAME
    FROM
        PRODUCT_CATEGORIES
    WHERE
        CATEGORY_ID = i_categoryId;

END SP_GETPRODUCTCATEGORY;